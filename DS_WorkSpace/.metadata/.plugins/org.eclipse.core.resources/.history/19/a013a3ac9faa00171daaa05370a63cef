package com.test.basics;

public class ArrayRakesh {

	public static void main(String args[]) {

		int count = 0;
		// int index =0;

		String[] strarr = { "a", "a", "c" };
		String input = "a";
		String strIndex = "";

		// for (String b:strarr)

		for (int index = 0; index <= strarr.length - 1; index++) {
			if (strarr[index].equals(input)) {
				count = count + 1;
				strIndex = strIndex + index;
			}
		}
		System.out.println(input + " -> " + count + strIndex);
	}
}
