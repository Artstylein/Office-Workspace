package com.rakesh.test;

public class Array {

	public static void main(String[] args) {
			int[] test = new int[10];
			test[0] = 0;
			test[1] = 2;
			test[2] = 4;
			test[3] = 6;
			test[4] = 8;
			
			for(int i=0;i<test.length;i++){
				 System.out.print(" " +test[i]);
			}
			
			
			System.out.println("2 D Array");
			int[][] arr = {{1,2,3},{2,3,4},{3,4,5}};
			
			
	}

}
